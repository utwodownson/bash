#!/bin/bash -

echo "=========bash pull========"
cd ~/bash
git pull
echo
echo

echo "=========leetcode pull========"
cd ~/leetcode
git pull
echo
echo

count=0

while true
do
    if [ $count -lt 3 ]; then
        let count++
        sleep 1
        echo -n "==>" 
    else
        echo
        exit 0
    fi
done

clear
cd ~
echo "=========finish pull github=========="



